{
    "builds": [
      {
        "src": "dsad-frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      },
      {
        "src": "dsad-backend/composer.json",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "dsad-frontend/build/$1" },
      { "src": "/api/(.*)", "dest": "dsad-backend/public/index.php" }
    ]
  }
  